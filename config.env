CONFIG_ENV=config.env
# ========================================
# КОНФИГУРАЦИЯ ДЛЯ СБОРКИ ЯДРА
# ========================================

# Источник ядра (URL репозитория)
KERNEL_SOURCE=https://github.com/alternoegraha/android_kernel_xiaomi_fog
# Ветка исходного кода ядра
KERNEL_SOURCE_BRANCH=15
# Конфигурационный файл ядра
KERNEL_CONFIG=fog_defconfig
# Название выходного файла ядра
KERNEL_ZIP_NAME=KernelSU-Makeine-fog.zip
# Архитектура (arm64 / arm / x86_64)
ARCH=arm64

# ========================================
# КОНФИГУРАЦИЯ KERNELSU
# ========================================
# Включить KernelSU
ENABLE_KERNELSU=true
# Версия/тег KernelSU
KERNELSU_TAG=v0.9.5
# Добавить конфигурацию kprobes
ADD_KPROBES_CONFIG=true
# Добавить конфигурацию OverlayFS
ADD_OVERLAYFS_CONFIG=true

# ========================================
# КОНФИГУРАЦИЯ КОМПИЛЯТОРОВ
# ========================================
# Использовать Clang (AOSP)
ENABLE_GCC_ARM64=true
# Использовать GCC для ARM32
ENABLE_GCC_ARM32=true
# Ветка Clang
CLANG_BRANCH=master-kernel-build-2022
# Версия Clang
CLANG_VERSION=r450784e

# ========================================
# ДОПОЛНИТЕЛЬНЫЕ КОМАНДЫ
# ========================================
# Дополнительные команды для сборки
EXTRA_CMDS=curl -LSs "https://raw.githubusercontent.com/tiann/KernelSU/main/kernel/setup.sh" | bash -s v0.9.5
